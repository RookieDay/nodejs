<pre><code>var fs =3D require('fs'),
    path =3D require('path'),
    http =3D require('http');

var MIME =3D {
    '.css': 'text/css',
    '.js': 'application/javascript'
};

function combineFiles(pathnames, callback) {
    var output =3D [];

    (function next(i, len) {
        if (i &lt; len) {
            fs.readFile(pathnames[i], function (err, data) {
                if (err) {
                    callback(err);
                } else {
                    output.push(data);
                    next(i + 1, len);
                }
            });
        } else {
            callback(null, Buffer.concat(output));
        }
    }(0, pathnames.length));
}

function main(argv) {
    var config =3D JSON.parse(fs.readFileSync(argv[0], 'utf-8')),
        root =3D config.root || '.',
        port =3D config.port || 80;

    http.createServer(function (request, response) {
        var urlInfo =3D parseURL(root, request.url);

        combineFiles(urlInfo.pathnames, function (err, data) {
            if (err) {
                response.writeHead(404);
                response.end(err.message);
            } else {
                response.writeHead(200, {
                    'Content-Type': urlInfo.mime
                });
                response.end(data);
            }
        });
    }).listen(port);
}

